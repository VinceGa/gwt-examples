<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>System.html</welcome-file>
  </welcome-file-list>
  

  <!-- Servlets -->
  <servlet>
    <servlet-name>servletRpc</servlet-name>
    <servlet-class>com.gawkat.gwt.system.server.RpcServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>servletRpc</servlet-name>
    <url-pattern>/system/system</url-pattern>
  </servlet-mapping>

</web-app>
