<module>

	<!-- Inherit the core Web Toolkit stuff.                  -->
	<inherits name='com.google.gwt.user.User'/>

	<!-- Specify the app entry point class.                   -->
	<entry-point class='com.tribling.gwt.RPC.adv.client.RPCadv'/>
  
  	<!-- the path is arbitrary or you could say the servlet makes it -->
  	<!-- class is the server class -->
  	<servlet path='/AdvService' class='com.tribling.gwt.RPC.adv.server.RPCServiceImpl'/>
</module>
