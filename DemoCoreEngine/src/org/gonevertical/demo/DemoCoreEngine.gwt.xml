<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='democoreengine'>

  <!-- inherit core - no need to inherit something that core did -->
  <inherits name='com.gawkat.core.Core'/>

 
  <!-- Other module inherits                                      -->
  <script src="../ga.js"/>


  <!-- Specify the app entry point class.                         -->
  <entry-point class='org.gonevertical.demo.client.DemoCoreEngine'/>

  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>
  <source path='shared'/>

</module>
