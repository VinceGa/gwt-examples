<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 1.6.4//EN" "http://google-web-toolkit.googlecode.com/svn/tags/1.6.4/distro-source/core/src/gwt-module.dtd">
<module rename-to='flashcard'>

	<!-- Google Analytics -->
	<script src="/ga.js" />

	<!-- Inherit the core Web Toolkit stuff.                        -->
	<inherits name='com.google.gwt.user.User' />

	<!-- CSS -->
	<inherits name='com.google.gwt.user.theme.standard.Standard' />
	<!-- <stylesheet src="FlashCard.css" /> TODO maybe use this here?? -->

	<!-- Other module inherits                                      -->
	<inherits name='com.google.gwt.gadgets.Gadgets' />


	<!-- Specify the app entry point class.                         -->
	<entry-point class='com.gawkat.flashcard.client.FlashCard' />
	
	
	<!-- TODO - This is needed to compile with appengine plugin, and nonhosted compile -->
	<!-- this has to be here, but won't load the gadget, need onload I think -->
	<entry-point class='com.gawkat.flashcard.client.gadget.FlashCard_Gadget' />
	
</module>
