<module>

	  
	 

      <!-- Inherit the core Web Toolkit stuff. -->
      <inherits name='com.google.gwt.user.User'/>
      
      <!-- Using MD5 key generator in this -->
      <inherits name="com.google.gwt.i18n.I18N"/>
      
      <inherits name='com.google.gwt.user.theme.standard.Standard'/>
	  

	  

      <!-- Other module inherits -->

	  <!-- Include the JS file. Put the file in Project Folder > src > public > sha1.js -->
	  <!-- http://pajhome.org.uk/crypt/md5/ - SHA-1 algorithms are secure hash functions. -->
	  <script src="sha1.js"/>


      <!-- Specify the app entry point class.                         -->
      <entry-point class='com.tribling.gwt.test.oauth.client.OAuth'/>
    
      <!-- Specify the application specific style sheet.              -->
      <stylesheet src='OAuth.css' />
      

	  <!-- servlet context - points to the class that the rpc request goes to     -->
	  <servlet path="/rpcServiceSession" class="com.tribling.gwt.test.oauth.server.RpcServiceImpl" />
	
	
</module>
