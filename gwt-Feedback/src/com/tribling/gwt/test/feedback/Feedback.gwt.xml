<module>

	<!-- Inherit the core Web Toolkit stuff.                  -->
	<inherits name='com.google.gwt.user.User'/>

	<!-- Specify the app entry point class.                   -->
	<entry-point class='com.tribling.gwt.test.feedback.client.Feedback'/>
  
    <!-- style -->
  	<stylesheet src="style.css" />
  	
  	<!-- servlet path must match up with the rpc init in code -->
  	<servlet class="com.tribling.gwt.test.feedback.server.FeedbackServiceImpl" path="/FeedbackService" />
  
</module>





	
	
