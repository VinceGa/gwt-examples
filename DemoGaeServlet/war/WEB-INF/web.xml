<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  
  <!-- Servlets -->
  <servlet>
    <servlet-name>rpcServlet</servlet-name>
    <servlet-class>org.gonevertical.servlet.server.RpcServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>rpcServlet</servlet-name>
    <url-pattern>/demogaeservlet/rpcService</url-pattern>
  </servlet-mapping>
  

  <servlet-mapping>
    <servlet-name>Serve</servlet-name>
    <url-pattern>/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Serve</servlet-name>
    <servlet-class>org.gonevertical.servlet.servlets.Servlet_Serve</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Serve</servlet-name>
    <url-pattern>/DemoGaeServlet.html</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Serve</servlet-name>
    <servlet-class>org.gonevertical.servlet.servlets.Servlet_Serve</servlet-class>
  </servlet>

</web-app>
