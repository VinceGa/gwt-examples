<module>

      <!-- Top Level project - user management system using oAuth     -->
	  <!-- refer to gwt-Core also!!!                                  -->
	 
	  <!-- Inherit the core Web Toolkit stuff.                        -->
      <inherits name='com.google.gwt.user.User'/>
	
	
      <!-- Inherit the default GWT style sheet.  			          -->
      <inherits name='com.google.gwt.user.theme.standard.Standard'/>

	  
      <!-- Other module inherits -->

      <!-- Specify the app entry point class.                         -->
      <!-- This is becoming a utility module -->
      <!-- <entry-point class='com.gawkat.gwt.oauth.client.OAuth'/>   -->
    
      <!-- Specify the application specific style sheet.              -->
      <stylesheet src='OAuth.css' />
      

	  <!-- servlet context - points to the class that the rpc request goes to     -->
	  <servlet path="/rpcServiceAccounts" class="com.gawkat.gwt.oauth.server.RpcServiceImpl" />
	
	
</module>
